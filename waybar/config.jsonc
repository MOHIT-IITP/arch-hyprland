{
    "layer": "top",
    "position": "top",
    "height": 10,
    "spacing": 0,
    "output": ["HDMI-A-1"],
    "margin-top": 5,
    "margin-bottom": 3,
    "margin-left": 10,
    "margin-right": 10,

    "modules-left": ["temperature","clock","custom/cava-music", "custom/playerctl",  "custom/cputemp"],
    "modules-center": ["hyprland/workspaces"],
    "modules-right": ["pulseaudio", "pulseaudio#microphone","bluetooth", "network", "cpu", "memory", "custom/screenshot","custom/colorpicker", "custom/notifications", "tray"],

    "hyprland/workspaces": {
        "format": " {icon} {windows}",
        "format-window-separator": " ",
        "on-scroll-up": "hyprctl dispatch workspace e+1",
        "on-scroll-down": "hyprctl dispatch workspace e-1",
        "window-rewrite-default": "",
        "window-rewrite-default": "",
        "window-rewrite": {
            "firefox": "Û∞àπ",
            "foot": "Û∞Ωí",
            "kitty": "ÓØä",
            "zen":"Ôâß",
            "alacritty": "ÓØá",
            "ghostty": "Óûï",
            "code": "Û∞®û",
            "cursor": "Û∞®û",
            "discord": "Ôáø",
            "vesktop": "Ôáø",
            "github": "Óúâ",
            "postman": "Ó°´",
            "telegram": "ÔãÜ",
            "google-chrome": "Ôâ®",
            "thunar": "Ôìî",
            "org.gnome.Nautilus": "Ôìî",
            "spotify": "Û∞ìá",
            "steam": "Û∞ìì",
            "org.pwmt.zathura": "Û∞àô"
        },
        "format-icons": {
            "1": "Û∞∞®",
            "2": "Û∞∞Ñ",
            "3": "Û∞∞ñ",
            "4": "Û∞∞ô",
            "5": "Û∞∞á",
            "6": "Û∞∞ä",
            "7": "Û∞∞ç",
            "8": "Û∞∞ì",
            "9": "Û∞∞ê",
            "10": "10",
            "urgent": "",
            "focused": "",
            "default": ""
        },
        "on-click": "activate",
        "all-outputs": true,
        "sort-by-number": true,
        "show-special": false 
    },

    "custom/colorpicker": {
        "format": "Óà´",
        "tooltip": "Pick a color",
        "on-click": "hyprpicker",
        "interval": "once"
    },

    "custom/screenshot": {
        "format": "Û∞πë",
        "tooltip": "Screenshot",
        "on-click": "grim -g \"$(slurp)\" - | wl-copy && paplay /usr/share/sounds/freedesktop/stereo/screen-capture.oga && notify-send 'Screenshot' 'Copied to clipboard'",
        "on-click-right": "grim -g \"$(slurp)\" ~/Pictures/Screenshots/$(date +%Y-%m-%d_%H-%M-%S).png && paplay /usr/share/sounds/freedesktop/stereo/screen-capture.oga && notify-send 'Screenshot' 'Saved to Pictures/Screenshots'",
        "on-click-middle": "grim ~/Pictures/Screenshots/$(date +%Y-%m-%d_%H-%M-%S).png && paplay /usr/share/sounds/freedesktop/stereo/screen-capture.oga && notify-send 'Screenshot' 'Fullscreen saved to Pictures/Screenshots'"
    },
    "clock": {
        "format": "Û∞•î {:%I:%M %p}",
        "format-alt": "{:%A, %B %d, %Y}",
        "tooltip-format": "<big>{:%Y %B}</big>\n<tt><small>{calendar}</small></tt>",
        "interval": 60
    },

    "cpu": {
        "format": "Û∞òö {usage}% ",
        "tooltip": true,
        "interval": 2
    },

    "memory": {
        "format": "Û∞çõ {}% ",
        "interval": 2
    },

    "network": {
        "format-wifi": "{essid} ({signalStrength}%) Û∞ñ©",
        "format-ethernet": "Û∞àÄ",
        "tooltip-format": "{ifname} via {gwaddr}",
        "format-linked": "{ifname} (No IP) Û∞àÄ",
        "format-disconnected": "Disconnected ‚ö†",
        "format-alt": "{ifname}: {ipaddr}/{cidr}",
        "on-click-right": "nm-connection-editor"
    },
    "custom/powermenu": {
        "format": "",
        "tooltip": false,
        "on-click": "~/.config/waybar/scripts/powermenu.sh",
        "interval": 0
    },
    "custom/cava-music": {
        "exec": "$HOME/.config/hypr/scripts/WaybarCava.sh",
        "format": "{}"
    },

    "custom/playerctl": {
        "format": "{}",
        "escape": true,
        "return-type": "json", 
        "max-length": 10,
        "exec": "$HOME/.config/waybar/scripts/mediaplayer.py 2> /dev/null",
        "on-click": "playerctl play-pause",
        "on-click-right": "playerctl stop",
        "on-scroll-up": "playerctl next",
        "on-scroll-down": "playerctl previous",
        "smooth-scrolling-threshold": 1,
        "tooltip": false
    },

    "pulseaudio": {
        "format": "{icon} {volume}% ",
        "format-bluetooth": "{volume}% {icon} Û∞ÇØ",
        "format-bluetooth-muted": "Û∞Ç≤ Û∞ñÅ {icon}",
        "format-muted": "Û∞ñÅ",
        "format-icons": {
            "headphone": "Û∞ãã",
            "hands-free": "Û∞ãé",
            "headset": "Û∞ãé",
            "phone": "Û∞è≤",
            "portable": "Û∞Ñù",
            "car": "Û∞Ñã",
            "default": ["Û∞ïø", "Û∞ñÄ", "Û∞ïæ"]
        },
        "on-click": "pavucontrol",
        "on-click-middle": "pactl set-sink-mute @DEFAULT_SINK@ toggle"
    },

    "pulseaudio#microphone": {
        "format": "{format_source}",
        "format-source": "Û∞ç¨ {volume}%",
        "format-source-muted": "Û∞ç≠",
        "on-click": "pactl set-source-mute @DEFAULT_SOURCE@ toggle",
        "on-click-right": "pavucontrol",
        "tooltip": true,
        "tooltip-format": "{format_source}",
        "scroll-step": 5
    },
    "custom/cputemp": {
        "format": "üå°Ô∏è {}¬∞C",
        "exec": "sensors | awk '/Tctl:/ {gsub(/\\+|¬∞C/, \"\", $2); print $2}'",
        "interval": 5,
        "return-type": "plain"
    },



    "bluetooth": {
        "format": "Ôäî {status}",
        "format-disabled": "", // an empty format will hide the module
        "format-connected": "Ôäî {num_connections}",
        "tooltip-format": "{device_alias}",
        "tooltip-format-connected": "Ôäî {device_enumerate}",
        "tooltip-format-enumerate-connected": "{device_alias}"
    },
    "custom/notifications": {
        "format": "{icon}",
        "format-icons": {
            "notification": "Û∞Çö",
            "none": "Û∞Çõ",
            "dnd-notification": "Û∞Çõ",
            "dnd-none": "Û∞Çõ",
            "inhibited-notification": "Û∞Çö",
            "inhibited-none": "Û∞Çõ",
            "dnd-inhibited-notification": "Û∞Çõ",
            "dnd-inhibited-none": "Û∞Çõ"
        },
        "return-type": "json",
        "exec-if": "which swaync-client",
        "exec": "swaync-client -swb",
        "on-click": "swaync-client -t -sw",
        "on-click-right": "swaync-client -d -sw",
        "escape": true
    },

    "tray": {
        "icon-size": 18,
        "spacing": 8
    }
}
